<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="com.zhao.elevator.activity.MainActivity">

    <Button
        android:text="注册管理员"

        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/xc_zc"
        android:layout_alignParentTop="true"
        android:layout_alignParentStart="true" />

    <Button
        android:text="登录"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_alignEnd="@+id/xc_zc"
        android:layout_marginTop="38dp"
        android:id="@+id/xc_denglu" />

    <Button
        android:text="用户注册"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/xc_denglu"
        android:layout_alignStart="@+id/xc_denglu"
        android:id="@+id/xc_zhuce2" />

    <Button
        android:text="主界面"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/xc_zhuce2"
        android:layout_alignParentStart="true"
        android:id="@+id/xc_zhujiemian" />

    <Button
        android:text="最快电梯"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/xc_zhujiemian"
        android:layout_alignStart="@+id/xc_zhuce2"
        android:id="@+id/xc_zuikuai" />

    <Button
        android:text="固定最快电梯"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/xc_zhujiemian"
        android:layout_alignStart="@+id/xc_zuikuai"
        android:layout_marginTop="37dp"
        android:id="@+id/xc_gudingzuikuai" />

    <Button
        android:text="Button"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerVertical="true"
        android:layout_alignParentStart="true"
        android:id="@+id/xc_yunxing" />

    <Button
        android:text="定时"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/xc_yunxing"
        android:layout_alignParentStart="true"
        android:id="@+id/xc_dinghsi" />

    <Button
        android:text="安全层禁"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/xc_dinghsi"
        android:layout_alignEnd="@+id/xc_dinghsi"
        android:id="@+id/xc_anquan" />

    <Button
        android:text="安全设置"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/xc_anquan"
        android:layout_alignStart="@+id/xc_gudingzuikuai"
        android:id="@+id/anquan_set" />

    <Button
        android:text="@string/qunti"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/anquan_set"
        android:layout_alignEnd="@+id/xc_anquan"
        android:id="@+id/xc_qunti" />

    <Button
        android:text="层禁申请"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/xc_qunti"
        android:layout_alignStart="@+id/anquan_set"
        android:id="@+id/button15" />

    <Button
        android:text="层禁2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_toEndOf="@+id/xc_zc"
        android:id="@+id/button20" />

    <Button
        android:text="权限申请"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/xc_zc"
        android:layout_toEndOf="@+id/xc_zhujiemian"
        android:id="@+id/button21" />

    <Button
        android:text="权限受理"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/button21"
        android:layout_toEndOf="@+id/xc_zhuce2"
        android:id="@+id/button22" />

    <Button
        android:text="管理员进入的主界面"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/xc_zhuce2"
        android:layout_toEndOf="@+id/xc_zuikuai"
        android:id="@+id/button26" />

    <Button
        android:text="个人信息"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/xc_zuikuai"
        android:layout_toEndOf="@+id/xc_zuikuai"
        android:id="@+id/button27" />

    <Button
        android:text="信息管理模块"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/xc_gudingzuikuai"
        android:layout_toEndOf="@+id/xc_gudingzuikuai"
        android:id="@+id/button28" />

    <Button
        android:text="Button"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/button28"
        android:layout_alignEnd="@+id/button28"
        android:layout_marginEnd="8dp"
        android:id="@+id/button29" />

    <Button
        android:text="地图"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/button29"
        android:layout_toEndOf="@+id/xc_gudingzuikuai"
        android:id="@+id/button30" />

</RelativeLayout>
